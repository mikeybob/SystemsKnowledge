#/home/mike		192.168.47.0/24(rw,sync,subtree_check) 10.99.10.0/24(rw,sync,subtree_check)
#/usr/share/nginx	192.168.47.0/24(rw,sync,subtree_check) 10.99.10.0/24(rw,sync,subtree_check)
